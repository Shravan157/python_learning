    "## Example 3: Calculate the Total cost of items in a shopping cart\n",
    "cart = [\n",
    "    {'name' : 'watch', 'price': 600 , 'quantity' : 4},\n",
    "    {'name' : 'backpack', 'price': 500 , 'quantity' : 3},\n",
    "    {'name' : 'shirt' , 'price': 900 , 'quantity' : 2},\n",
    "]\n",
    "def calculate_cart_total(cart):\n",
    "    total = 0\n",
    "    for item in cart:\n",
    "        price = item['price']\n",
    "        quantity = item['quantity']\n",
    "        price_quantity = quantity * price\n",
    "        total = total + price_quantity\n",
    "    return total\n",
    "\n",
    "print(calculate_cart_total(cart))\n"
   ],
